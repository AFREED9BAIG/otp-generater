      "source": [
        "## Project: OTP Generator\n",
        "# Generate a 6 character OTP\n",
        "import random as r\n",
        "import string\n",
        "length = 6 \n",
        "otp = ''\n",
        "characters = string.ascii_letters + string.digits\n",
        "\n",
        "for i in range(length):\n",
        "  otp = otp + r.choice(characters)\n",
        "\n",
        "print(\"OTP:\", otp)"
      ]
    }
  ]
